<h1>Productos</h1>

<table id="main">
    <tr>
        <th>Titulo</th>
        <th>Descripcion</th>
        <th>Precio</th>
        <th>Imagen</th>
        <th>Codigo</th>
        <th>Stock</th>
    </tr>
    {{#each products}}
        <tr>
            <td>{{ this.title }}</td>
            <td>{{ this.description }}</td>
            <td>{{ this.price }}</td>
            <td>{{ this.thumbnail }}</td>
            <td>{{ this.code }}</td>
            <td>{{ this.stock }}</td>
        </tr>
    {{/each}}
</table>