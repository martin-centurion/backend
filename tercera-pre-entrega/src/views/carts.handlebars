<header class="header container">
        <h1 class="header__logo">Swatch</h1>
        <nav class="nav-menu">
            <button class="open-menu" id="open-menu"><i class="bi bi-list"></i></button>
            <ul id="nav-list" class="nav-list">
                <button class="close-menu" id="close-menu"><i class="bi bi-x-lg"></i></button>
                <li class="nav-item"><a class="nav-link" href="#">Productos</a></li>
                <li class="nav-item"><a class="nav-link" href="#">Nosotros</a></li>
            </ul>    
        </nav>
        <nav class="nav-icon">
              <li class="nav-item"><a href="http://localhost:8080/register"><i class="bi bi-person"></i></a></li>
              <li class="nav-item"><a href="/logout"><i class="bi bi-person-dash"></i></a></li>
              <li class="nav-item"><a href="/carts"><i class="bi bi-bag"></i></a></li>
        </nav>
    </header>
    <main>
        <section class="banner">
            <div class="container">
                <div class="banner__content">
                    <h1 class="banner__content-titulo">Bienvenido, {{userName}} - {{userRole}}</h1>
                </div>
            </div>
        </section>
        <section class="container">
            <div class="productos__content container">
                <div class="productos__content-title">
                    <h2>Carrito</h2>
                </div>
            </div>

            <div>
                <div class="productos-img">
                  {{#each payload}}
                        <article>
                            <p>{{this.text}}</p>
                            <p>{{data.products.title}}</p>
                        </article>
                  {{/each}}  
                </div>
            </div>
            
        </section>
    </main>
    <footer class="footer">
    </footer>